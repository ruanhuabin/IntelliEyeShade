<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 

  This mapping demonstrates content-based discrimination for the
  table-per-hierarchy mapping strategy, using a formula
  discriminator.

-->

<hibernate-mapping>
	
	<class name="entity.DetectDetail"  table="DETECTDETAIL">
		
		<id name="did" type="java.lang.String">
			<generator class="assigned"/>
		</id>
		
		<property name="focusDegrees" type="java.lang.String" length="1048576"/>
		<property name="relaxDegrees" type="java.lang.String" length="1048576"/>
		<property name="heartRates" type="java.lang.String" length="1048576"/>
		<property name="heartRateVariations" type="java.lang.String" length="1048576"/>
		<property name="brainRawData" type="java.lang.String" length="1048576"/>
		<property name="pulseWaveData" type="java.lang.String" length="1048576"/>
		<property name="timeStamp" type="java.lang.String" length="128"/>
		
	</class>

	


</hibernate-mapping>
